@model IEnumerable<WebsiteCommon.Applications.EmailApplication>

@{
    ViewBag.Title = "Email Settings";
}
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">Email Settings</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            Email users
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="dataTable_wrapper">
                                <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                    <thead>
                                        <tr>
                                            <th>Client</th>
                                            <th>Application</th>
                                            <th>To Addresses</th>
                                            <th>Origin Urls</th>
                                            <th>IP restrictions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var e in Model)
                                        {
                                            <tr class="odd gradeX">
                                                <td>@e.CustomerId</td>
                                                <td>???</td>
                                                <td>@e.ToAddresses</td>
                                                <td>@e.OriginUrls</td>
                                                <td>@e.IPAccessRestrictions</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                            <!-- /.table-responsive -->
                            <div class="well">
                                <h4>Add new email client</h4>
                            </div>
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
        </div>
        <!-- /#page-wrapper -->



